{% extends "layout.dtl" %}

{% block content %}

<h1>{% trans "args_list_title" %}</h1>
<br>
{% if arg_list %}
	<table border="1">
		{% for arg, value in arg_list %}	
		<tr>
			<td>{{ arg }}</td><td>{{ value }}</td>
		</tr>
		{% endfor %}
	</table>
{% else %}
	<p>{% trans "args_list_empty_list" %}</p>
{% endif %}

{% endblock %}